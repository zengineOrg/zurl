{
    "name": "zengine/zurl",
    "description": "Perform various self diagnosis tests on your Laravel application.",
    "keywords": [
        "zengine",
        "curl wrapper"
    ],
    "homepage": "https://github.com/zengineDev/zurl",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Bartelt",
            "email": "christian@zengine.org",
            "homepage": "https://zengine.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.8.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Zengine\\Zurl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zengine\\Zurl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zengine\\Zurl\\ZurlServiceProvider"
            ]
        }
    }
}
